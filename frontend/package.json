{
  "name": "my-vue-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "format": "prettier --write \"src/**/*.vue\"",
    "format-js": "prettier --write \"src/**/*.js\"",
    "format-all": "npm run format && npm run format-js"
  },
  "dependencies": {
    "ant-design-vue": "^4.0.0",
    "body-parser": "^1.20.2",
    "core-js": "^3.8.3",
    "cors": "^2.8.5",
    "crypto-browserify": "^3.12.0",
    "element-plus": "^2.3.8",
    "express": "^4.18.2",
    "mysql": "^2.18.1",
    "mysql2": "^3.5.2",
    "sequelize": "^6.32.1",
    "stream-browserify": "^3.0.0",
    "timers-browserify": "^2.0.12",
    "url": "^0.11.1",
    "util": "^0.12.5",
    "vue": "^3.2.26",
    "vue-router": "^4.2.4",
    "vue-sidebar-menu-akahon": "^1.0.2",
    "vuex": "^4.1.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.19.3/xlsx-0.19.3.tgz"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "prettier": "3.0.0",
    "prettier-eslint": "^15.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
